  DATAITEMS
  {
    { PROPERTIES
      {
        DataItemTable=Table50011;
        DataItemTableView=WHERE(Status=FILTER(<>ACTS));
        OnPreDataItem=BEGIN
                        Firmendaten.GET;
                        Firmendaten.CALCFIELDS("ISO Logo");
                      END;

        OnAfterGetRecord=BEGIN
                           CurrReport.NEWPAGE;
                         END;

        ReqFilterFields=Wartungsvertragsnr.;
      }
      SECTIONS
      {
      }
       }
    { PROPERTIES
      {
        DataItemIndent=1;
        DataItemTable=Table50024;
        DataItemTableView=SORTING(Gruppierung,Basisger„t)
                          ORDER(Descending)
                          WHERE(Art=FILTER(Service),
                                Inaktiv=FILTER(No),
                                SUP Artikelnr.=FILTER(<>CHAMPS-RE_INSTATE#01&<>RE_INSTATEMENT_FEE));
        OnPreDataItem=BEGIN
                        CurrReport.PAPERSOURCE(3);

                        Nummer := 0;
                        vkkopf_text := '';
                        status_text := '';
                      END;

        OnAfterGetRecord=BEGIN
                           IF Shipment.GET("Serviceleist. & Leihstellungen"."Nr.") THEN
                             BEGIN
                               IF (Shipment."Sell-to Country/Region Code" <> 'DE') THEN
                               //AND(Shipment."Sell-to Country/Region Code" <>'AT')
                               //AND (Shipment."Sell-to Country/Region Code" <>'CH'))THEN
                                 BEGIN
                                   CurrReport.LANGUAGE(1033); // Englischer L„ndercode  (ENU Englisch(USA))
                                 END
                               ELSE
                                 BEGIN
                                   CurrReport.LANGUAGE(1031); // Deutscher L„ndercode
                                 END;
                             END;

                           //Suche nach 1. Basisger„t zum Andruck des Benuters + PW
                           ZugangsdatenAndruck.RESET;
                           ZugangsdatenAndruck.SETFILTER(Art,'%1',ZugangsdatenAndruck.Art :: Service);
                           ZugangsdatenAndruck.SETFILTER(Inaktiv,'%1',FALSE);
                           ZugangsdatenAndruck.SETFILTER(Basisger„t,'%1',TRUE);
                           ZugangsdatenAndruck.SETFILTER("SUP Artikelnr.",'%1',"Serviceleist. & Leihstellungen"."SUP Artikelnr.");
                           ZugangsdatenAndruck.SETFILTER("Wartungsvertragsnr.",'%1',"Serviceleist. & Leihstellungen"."Wartungsvertragsnr.");
                           IF ZugangsdatenAndruck.FINDFIRST THEN
                             BEGIN
                               benutzer := ZugangsdatenAndruck.Benutzername;
                               pw := ZugangsdatenAndruck.Passwort;
                             END
                           ELSE
                             //Wartungsvertrag verfgt ber kein Basisger„t
                             BEGIN
                               ZugangsdatenAndruck.RESET;
                               ZugangsdatenAndruck.SETFILTER(Art,'%1',ZugangsdatenAndruck.Art :: Service);
                               ZugangsdatenAndruck.SETFILTER(Inaktiv,'%1',FALSE);
                               ZugangsdatenAndruck.SETFILTER("SUP Artikelnr.",'%1',"Serviceleist. & Leihstellungen"."SUP Artikelnr.");
                               ZugangsdatenAndruck.SETFILTER("Wartungsvertragsnr.",'%1',"Serviceleist. & Leihstellungen"."Wartungsvertragsnr.");
                               IF ZugangsdatenAndruck.FINDFIRST THEN
                                 BEGIN
                                   benutzer := ZugangsdatenAndruck.Benutzername;
                                   pw := ZugangsdatenAndruck.Passwort;
                                 END
                               ELSE
                                BEGIN
                                  benutzer := "Serviceleist. & Leihstellungen".Benutzername;
                                  pw := "Serviceleist. & Leihstellungen".Passwort;
                                END;
                             END;

                           sup_artikelnr := "Serviceleist. & Leihstellungen"."SUP Artikelnr.";
                           Artikel.GET("SUP Artikelnr.");
                           sup_artikelnr_lang := Artikel."No. 2";

                           IF ((STRPOS(sup_artikelnr_lang,'HW') <> 0) OR (STRPOS(sup_artikelnr_lang,'SW') = 0)
                           OR (sup_artikelnr_lang = 'CHAMPS-SUPP-8X5/YR' ) OR (STRPOS(sup_artikelnr_lang,'RPLC') <> 0))
                           AND (STRPOS(sup_artikelnr_lang,'SOFTWARE') = 0)
                           AND (STRPOS(sup_artikelnr_lang,'ENH-SUPP-PROG') = 0) THEN
                             BEGIN
                               benutzer := '-';
                               pw := '-';
                             END;
                         END;

        GroupTotalFields=Gruppierung;
        DataItemLink=Wartungsvertragsnr.=FIELD(Wartungsvertragsnr.);
      }
      SECTIONS
      {
        { PROPERTIES
          {
            SectionType=Header;
            PrintOnEveryPage=Yes;
            SectionWidth=18541;
            SectionHeight=12100;
            KeepWithNext=No;
            OnPreSection=BEGIN
                           LetzteServicePos.RESET;
                           LetzteServicePos.SETFILTER("Wartungsvertragsnr.",'%1',"Wartungsvertragsnr.");
                           LetzteServicePos.SETFILTER(Art,'%1',LetzteServicePos.Art :: Service);
                           IF LetzteServicePos.FINDLAST THEN
                             BEGIN
                               IF STRPOS(LetzteServicePos."SUP Artikelnr.",'CHAMPS') <> 0 THEN
                                 BEGIN
                                   andruck_champs := TRUE;
                                   IF CurrReport.LANGUAGE = 1031 THEN
                                     vkkopf_text := 'Champs Servicevertrag'
                                   ELSE
                                     vkkopf_text := 'Champs Service Agreement';
                                 END
                               ELSE
                                 BEGIN
                                   IF STRPOS(LetzteServicePos."SUP Artikelnr.",'ACHAT') <> 0 THEN
                                     BEGIN
                                       andruck_champs := TRUE;
                                       IF CurrReport.LANGUAGE = 1031 THEN
                                         vkkopf_text := 'ACHAT Servicevertrag'
                                       ELSE
                                         vkkopf_text := 'ACHAT Service Agreement';
                                     END
                                   ELSE
                                     BEGIN
                                       andruck_champs := FALSE;
                                       vkkopf_text := 'Software Upgrade Protection';
                                     END;
                                 END;
                             END;

                           KopfdatenAndruck.RESET;
                           KopfdatenAndruck.SETFILTER(Art,'%1',KopfdatenAndruck.Art :: Service);
                           KopfdatenAndruck.SETFILTER("Wartungsvertragsnr.",'%1',"Wartungsvertragsnr.");
                           KopfdatenAndruck.SETFILTER(Inaktiv,'%1',FALSE);
                           IF KopfdatenAndruck.FINDLAST THEN
                             BEGIN
                             END;

                           auftragsnr := KopfdatenAndruck."Auftragsnr.";
                           debitornr := KopfdatenAndruck."Debitornr.";
                           datum := KopfdatenAndruck.Belegdatum;

                           ihre_ref := '';
                           ansp_telefon := '';
                           ansp_fax:= '';
                           ansp_anrede := '';
                           ansp_name := '';
                           ansp_email := '';

                           ArchHeader.RESET;
                           ArchHeader.SETFILTER(ArchHeader."No.",'%1',KopfdatenAndruck."Auftragsnr.");
                           IF ArchHeader.FINDFIRST THEN
                             BEGIN
                               IF ArchHeader."External Document No." <> '' THEN ihre_ref := ArchHeader."External Document No."+' ';
                               IF ArchHeader."Your Reference"  <> '' THEN ihre_ref := ihre_ref + ArchHeader."Your Reference";

                               Land_Region.GET(ArchHeader."Sell-to Country/Region Code");

                               IF Verk„ufer.GET(ArchHeader."Betreuer Innendienst") THEN
                                 BEGIN
                                    IF  CurrReport.LANGUAGE = 1031 THEN
                                      BEGIN
                                        ansp_fax := Verk„ufer."Fax no.";
                                        ansp_anrede := Verk„ufer.Ansprech;
                                        ansp_telefon := Verk„ufer."Phone No.";
                                      END
                                    ELSE
                                      BEGIN
                                        ansp_fax := Verk„ufer.Fax_international;
                                        ansp_anrede := Verk„ufer.Ansprech_eng;
                                        ansp_telefon := Verk„ufer.Tel_international;
                                      END;
                                    ansp_name := Verk„ufer."first name" + ' ' + Verk„ufer.Name;
                                    ansp_email := Verk„ufer."E-Mail";
                                 END;
                             END
                           ELSE
                             BEGIN
                               SalesHeader.RESET;
                               SalesHeader.SETFILTER(SalesHeader."No.",'%1',KopfdatenAndruck."Auftragsnr.");
                               IF SalesHeader.FINDFIRST THEN
                                 BEGIN
                                   IF SalesHeader."External Document No." <> '' THEN ihre_ref := SalesHeader."External Document No."+' ';
                                   IF SalesHeader."Your Reference"  <> '' THEN ihre_ref := ihre_ref + SalesHeader."Your Reference";

                                   Land_Region.GET(SalesHeader."Sell-to Country/Region Code");

                                   IF Verk„ufer.GET(SalesHeader."Betreuer Innendienst") THEN
                                     BEGIN
                                       IF  CurrReport.LANGUAGE = 1031 THEN
                                         BEGIN
                                           ansp_fax := Verk„ufer."Fax no.";
                                           ansp_anrede := Verk„ufer.Ansprech;
                                           ansp_telefon := Verk„ufer."Phone No.";
                                         END
                                       ELSE
                                         BEGIN
                                           ansp_fax := Verk„ufer.Fax_international;
                                           ansp_anrede := Verk„ufer.Ansprech_eng;
                                           ansp_telefon := Verk„ufer.Tel_international;
                                         END;
                                       ansp_name := Verk„ufer."first name" + ' ' + Verk„ufer.Name;
                                       ansp_email := Verk„ufer."E-Mail";
                                     END;
                                 END;
                             END;

                           FormatAddr.FormatAddr(AndruckFeld,KopfdatenAndruck.Debitorname,KopfdatenAndruck."Name 2",KopfdatenAndruck.Kontakt,
                           KopfdatenAndruck.Adresse,KopfdatenAndruck."Adresse 2",KopfdatenAndruck.Ort,KopfdatenAndruck."PLZ-Code",'',Land_Region.Code);
                         END;

          }
