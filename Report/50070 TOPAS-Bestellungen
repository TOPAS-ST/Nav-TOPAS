        { PROPERTIES
          {
            SectionType=Body;
            SectionWidth=19000;
            SectionHeight=400;
            OnPreSection=BEGIN
                           CurrReport.SHOWOUTPUT(((EinkZeile.Type = EinkZeile.Type::Item) OR (EinkZeile.Type = EinkZeile.Type::"Fixed Asset")) AND
                           ("Purchase Line"."Quotenr. Hersteller" <> ''));
                           IF "Purchase Line"."Quotenr. Hersteller" <> '' THEN
                           BEGIN
