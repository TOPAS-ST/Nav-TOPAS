                                                                  Ekzeile.SETFILTER(Ekzeile."No.",'%1',"No.");
                                                                  IF Ekzeile.FINDFIRST THEN
                                                                    REPEAT
                                                                      Ekzeile."Artikelnr. 2" := "No. 2";
                                                                      Ekzeile.MODIFY;
                                                                    UNTIL Ekzeile.NEXT = 0;

                                                                  Champs.RESET;
                                                                  Champs.SETFILTER(Champs."Artikelnr. 2",'%1',"No. 2");
