                                                                  IF Vkz.FINDFIRST THEN
                                                                  REPEAT
                                                                     Vkz."Manufacturer Code" := "Manufacturer Code";
                                                                     Vkz.MODIFY;
                                                                  UNTIL Vkz.NEXT = 0;

                                                                  Ekz.SETFILTER(Ekz.Type,'%1',Ekz.Type::Item);
                                                                  Ekz.SETFILTER(Ekz."No.",'%1',"No.");
                                                                  IF Ekz.FINDFIRST THEN
