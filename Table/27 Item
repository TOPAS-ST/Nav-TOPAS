                                                                IF T_Item.FINDFIRST THEN
                                                                  ERROR(STRSUBSTNO(Text1000000,"No. 2",T_Item."No.",T_Item.Description));

                                                                //TOPAS 20.11.08 ST
                                                                IF ("No. 2" <> xRec."No. 2") AND ("No. 2" <> '') AND ("No." <> '') THEN BEGIN
                                                                  Projartikel.RESET;
                                                                  Projartikel.SETFILTER(Projartikel."Artikelnr.",'%1',"No.");
                                                                  IF Projartikel.FINDFIRST THEN
                                                                  REPEAT
                                                                    Projartikel."Artikelnr. 2" := "No. 2";
                                                                    Projartikel.MODIFY;
                                                                  UNTIL Projartikel.NEXT = 0;

